    <Compile Remove="HTTP\**" />
    <Compile Remove="DNS\**" />
    <Compile Remove="NNTP\**" />
    <Compile Remove="WebDav\**" />
    <Compile Remove="RTP\**" />
    <Compile Remove="STUN\**" />
    <Compile Remove="UPnP\**" />
    <Compile Remove="SNTP\**" />
    <Compile Remove="SDP\**" />
    <Compile Remove="ICMP\**" />
    <Compile Remove="Media\**" />
    <Compile Remove="vCard\**" />
    <Compile Remove="_Debug\**" />
    <Compile Remove="_Obsolete\**" />
    <Compile Remove="URI\SIP_Uri.cs" />
    <Compile Remove="URI\TEL_Uri.cs" />
    <Compile Remove="IMAP\Client\**" />
  </ItemGroup>

